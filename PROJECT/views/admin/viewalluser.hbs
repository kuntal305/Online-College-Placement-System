<link href="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" integrity="sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU" crossorigin="anonymous">
<link rel="stylesheet" href="/stylesheets/adminprofile/searchbar.css">

{{>adminprofilenavbar}}

<div class="container text-center">
    <h1 class="mt-5 text-white font-weight-light">View All Users</h1>
</div>


{{#if status}}
    {{#if hasAnyUser}}
    
        <h2 class="mt-5 text-white font-weight-light" style="text-align: center"> USER LIST</h2><br>
        

        
            
            
                
                    
            <table class="table table-hover table-striped table-responsive">
                
                    <thead class="thead-dark">
                    <tr>
                        <th>NAME</th>
                        <th>DOB</th>
                        <th>GENDER</th>
                        <th>STREAM</th>
                        <th>EMAIL</th>
                        <th>MOBILENO</th>
                        <th>SECONDARY SCHOOL email</th>
                        <th>10th PASSING YEAR</th>
                        <th>10th BOARD</th>
                        <th>10th MARKS</th>
                        <th>HIGH SECONDARY SCHOOL</th>
                        <th>12th PASSING YEAR</th>
                        <th>12th BOARD</th>
                        <th>12th MARKS</th>
                        <th>UNIVERSITY</th>
                        <th>SEM 1</th>
                        <th>SEM 2</th>
                        <th>SEM 3</th>
                        <th>SEM 4</th>
                        <th>SEM 5</th>
                        <th>SEM 6</th>
                        <th>SEM 7</th>
                        <th>SEM 8</th>
                                        
                    </tr>
                </thead>

                
                
                {{#each getdocumentfrommongodb}}
                <tr>
                    <td>{{ this.firstname }} {{this.lastname}}</td>
                    <td>{{ this.dob }}</td>
                    <td>{{this.gender}}</td>
                    <td>{{this.stream}}</td>
                    <td>{{this.email}}</td>
                    <td>{{ this.mobileno }}</td>
                    <td>{{this.schoolname1}}</td>
                    <td>{{this.passingyear1}}</td>
                    <td>{{ this.board1}}</td>
                    <td>{{ this.marks1 }}</td>
                    <td>{{ this.schoolname2 }}</td>
                    <td>{{ this.passingyear2 }}</td>
                    <td>{{ this.board2}}</td>
                    <td>{{ this.marks2 }}</td>
                    <td>{{ this.university}}</td>
                    <td>{{ this.sem1}}</td>
                    <td>{{ this.sem2}}</td>
                    <td>{{ this.sem3}}</td>
                    <td>{{ this.sem4}}</td>
                    <td>{{ this.sem5}}</td>
                    <td>{{ this.sem6}}</td>
                    <td>{{ this.sem7}}</td>
                    <td>{{ this.sem8}}</td>
                        
                </tr>
                {{/each}}
            </table>
    {{else}}
    <h1 class="mt-5 text-white font-weight-light"> NO USER FOUND </h1>
        
    {{/if}}
{{/if}}



<script src="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.15.0/umd/popper.min.js"></script>


<!---------------------------------------------------------- The Modal ------------------------------------------------------->


<div class="modal fade" id="myModal" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
    <div class="modal-content">
    
        <!-- Modal Header -->
        <div class="modal-header">
        <h4 class="modal-title" style="color: black;">STUDENT DETAILS</h4>
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        </div>
        
        <!-- Modal body -->
        <div class="modal-body">
            <table id="myTable" class="table table-dark table-striped" style="color: black">
                <tr>
                    <td>Name</td><td>{{this.firstname}}  {{this.lastname}}</td>
                </tr>
                <tr>
                    <td>DOB</td><td>{{this.dob}}</td>
                </tr>
                <tr>
                    <td>GENDER</td><td>{{this.gender}}</td>
                </tr>
                <tr>
                    <td>STREAM</td><td>{{this.stream}}</td>
                </tr>
                <tr>
                    <td>EMAIL</td><td>{{this.email}}</td>
                </tr>
                <tr>
                    <td>MOBILE NO.</td><td>{{this.mobileno}}</td>
                </tr>
                <tr>
                    <td>SECONDARY SCHOOL NAME</td><td>{{this.schoolname1}}</td>
                </tr>
                <tr>
                    <td>PASSING YEAR</td><td>{{this.passingyear1}}</td>
                </tr>
                <tr>
                    <td>BOARD</td><td>{{this.board1}}</td>
                </tr>
                <tr>
                    <td>MARKS</td><td>{{this.marks1}}</td>
                </tr>
                <tr>
                    <td>HIGH SECONDARY SCHOOL NAME</td><td>{{this.schoolname2}}</td>
                </tr>
                <tr>
                    <td>PASSING YEAR</td><td>{{this.passingyear2}}</td>
                </tr>
                <tr>
                    <td>BOARD</td><td>{{this.board2}}</td>
                </tr>
                <tr>
                    <td>MARKS</td><td>{{this.marks2}}</td>
                </tr>
                <tr>
                    <td>UNIVERSITY</td><td>{{this.university}}</td>
                </tr>
                <tr>
                    <td>1st SEM</td><td>{{this.sem1}}</td>
                </tr>
                <tr>
                    <td>2nd SEM</td><td>{{this.sem2}}</td>
                </tr>
                <tr>
                    <td>3rd SEM</td><td>{{this.sem3}}</td>
                </tr>
                <tr>
                    <td>4th SEM</td><td>{{this.sem4}}</td>
                </tr>
                <tr>
                    <td>5th SEM</td><td>{{this.sem5}}</td>
                </tr>
                <tr>
                    <td>6th SEM</td><td>{{this.sem6}}</td>
                </tr>
                <tr>
                    <td>7th SEM</td><td>{{this.sem7}}</td>
                </tr>
                <tr>
                    <td>8th SEM</td><td>{{this.sem8}}</td>
                </tr>
                
            </table>
        </div>
        
        <!-- Modal footer -->
        <div class="modal-footer">
        <button type="button" class="btn btn-danger" data-dismiss="modal">DELETE</button>
        </div>
        
    </div>
    </div>
</div>

